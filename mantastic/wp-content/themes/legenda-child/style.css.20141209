@media (max-width: 979px) {

	#home_block1 .span4, #home_block1 .span8, #home_block2 .span9, #home_block2 .span2,
	#home_block4 .span9, #home_block4 .span3 {
		width: 100%;
		float: none;
	}
	#home_block1 h1{
		font-size: 45px;
	}
	#home_block1 h5, #home_block1 h1{
		text-align: center;
	}
	#five_letters{
		font-size: 17px;
	}

	#home_block2{
		background-size: 300% 300%;
		margin-top: 0;
	}

	#home_block3{
		display: none;
	}

	#home_block4{
		margin-top: 20px;
		background-size: 300% 300%
	}

	#home_block4 p{
		padding-bottom: 0;
	}

	#home_block5{
		margin-top: -35px;
	}
	#home_block5 .last{
		display: none;
	}

	#home_block6{
		margin-top: 0;
		background-size: 300% 300%;
	}

	#home_block6 .container{
		top: 0;
	}

	#home_block7{
		background-size: 300% 300%;
		margin-top: 0;
	}

	#home_block7 .container{
		top: 0;
	}

	#home_block7 .container .content_block{
		margin-top: 0;
	}
}

@media (max-width: 768px) {
	#home_block1 h1{
		font-size: 30px;
	}
	#home_block1 h5{
		font-size: 20px;
	}

	#five_letters{
		margin-left: 0;
	}

	#watch_video{
		background: url("images/play_gray.png") no-repeat scroll 0 0 rgba(0, 0, 0, 0);
	    display: block;
	    font-size: 19px;
	    height: 100px;
	    line-height: 100px;
	    padding-left: 100px;
	    padding-top: 0;
	    text-shadow: 1px 1px #555;
	    text-transform: uppercase;
	}

	.block5_ul{
		padding-left: 0;
	}

	.block5_ul li{
		width: 100%;
	}

	#home_block6 .span2b{
		width: 100% !important;
		float: none !important;
	}
