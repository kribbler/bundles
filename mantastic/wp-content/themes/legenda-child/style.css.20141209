@import url("../legenda/style.css");
@import url(http://fonts.googleapis.com/css?family=Special+Elite);
@import url(http://fonts.googleapis.com/css?family=Roboto);
@import url(http://fonts.googleapis.com/css?family=Roboto+Condensed:400,300,700);
@import url(http://fonts.googleapis.com/css?family=Oswald:400,300);
/* =Theme customization starts here
-------------------------------------------------------------- */

body, h1, h2, h3, h4, h5, h6{
	font-family: "Roboto Condensed", sans-serif !important;
	text-transform: none;
}

#fade-left{
	position: absolute;
	height: 100%;
	width: 237px;
	background: url(images/fade-left.png) left top repeat-y;
	z-index: 1000;
	left: -100px;
}

#fade-right{
	position: absolute;
	height: 100%;
	width: 237px;
	background: url(images/fade-right.png) right top repeat-y;
	right: -100px;
	z-index: 1000;
}
.fixed-header{
	background: url(images/bg_header_bar.png) left top repeat-x;
}

.blueish{
	color: #5dbfd8;
}

.fixed-header-area{
	display: none;
}

.main-nav, .main-nav .double-border, .sidebar-widget .widget-title{
	border-top: none !important;
	border-bottom: none !important;
}

#header_top_text p{
	font-size: 26px !important;
	text-align: right;
	color: #fff;
	font-weight: 700;
}

.top-bar{
	background: url(images/bg_top_bar.png) left top repeat-x !important;
	z-index: 10;
}

.top-bar .row-fluid{
	background: url(images/bg_top_bar_links.png) right top no-repeat !important;
	margin-top: -4px;
	padding: 14px 10px;
}

.top-bar ul{
	list-style: none;
}

.top-bar ul li{
	float: right;
	color: #fff;
	font-family: "Roboto Condensed", sans-serif !important;
	font-size: 14px;
	font-weight: 700;
	text-transform: uppercase;
	margin: 0 3px;
}

.bubblish{
	background: url(images/bg_inner_pages.png) center top repeat-y;
}
.main-nav{
	background:none !important;
	margin-top: -35px;
}
.main-nav .menu{
	margin-right: 0 !important;
	background: url(images/bg_menu.png) left top no-repeat;
	padding: 0 20px;
}
.main-nav .menu > li{
	padding: 0px !important;
}
.main-nav .menu li a{
	font-weight: 700;
	font-style: italic;
	font-family: "Roboto Condensed", sans-serif;
	font-size: 16px;
	color: #1a1a1a !important;
	padding-top: 17px !important;
}

.blue_ul{
	list-style: square;
}

.blue_ul li{
	list-style-image: url(images/blue_li.png);
	color: #fff;
	font-size: 15px;
}

.header-type-3 .logo-with-menu + div, .header-type-4 .logo-with-menu + div{
	vertical-align: bottom;
}

